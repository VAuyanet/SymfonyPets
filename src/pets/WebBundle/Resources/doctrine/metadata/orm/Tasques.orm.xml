    <id name="idTasca" type="integer" column="idTasca">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="titol" type="text" column="titol" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="descripcio" type="text" column="descripcio" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dataInici" type="date" column="data_inici" nullable="false"/>
    <field name="dataFinal" type="date" column="data_final" nullable="false"/>
    <field name="estat" type="text" column="estat" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="prioritat" type="text" column="prioritat" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="departament" type="text" column="departament" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
